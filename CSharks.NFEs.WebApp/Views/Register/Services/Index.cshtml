            <span asp-validation-for="aliquota_item_lista_servico" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="situacao_tributaria" class="control-label"></label>
            <input asp-for="situacao_tributaria" class="form-control" />
            <span asp-validation-for="situacao_tributaria" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="valor_tributavel" class="control-label"></label>
            <input asp-for="valor_tributavel" class="form-control" />
            <span asp-validation-for="valor_tributavel" class="text-danger"></span>
        </div>
        <div class="form-group">
            <button type="submit" class="btn btn-primary mt-4">Create</button>
        </div>
    </form>

</section>

