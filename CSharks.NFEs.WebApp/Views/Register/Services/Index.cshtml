            <input asp-for="tributa_municipio_prestador" class="form-control" />
            <span asp-validation-for="tributa_municipio_prestador" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="codigo_local_prestacao_servico" class="control-label"></label>
            <input asp-for="codigo_local_prestacao_servico" class="form-control" />
            <span asp-validation-for="codigo_local_prestacao_servico" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="codigo_item_lista_servico" class="control-label"></label>
            <input asp-for="codigo_item_lista_servico" class="form-control" />
